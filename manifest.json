{
  "name": "Encadrement",
  "version": "5.1.4",
  "description": "Soyez informés du prix maximum des annonces (basé sur l'encadrement des loyers) sur tous vos sites d'annonces préférés",
  "permissions": [
    "*://encadrement-loyers.herokuapp.com/*"
  ],
  "page_action": {
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "48": "images/favicon-48x48.png",
      "128": "images/favicon-128x128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.seloger.com/*",
        "https://www.leboncoin.fr/*",
        "https://www.jinka.fr/*",
        "https://www.pap.fr/*",
        "https://www.logic-immo.com/*",
        "https://immobilier.lefigaro.fr/*",
        "https://www.facebook.com/marketplace/*",
        "https://www.orpi.com/*"
      ],
      "js": [
        "helper/api.helper.js",
        "service/seloger.service.js",
        "service/leboncoin.service.js",
        "service/loueragile.service.js",
        "service/pap.service.js",
        "service/logicimmo.service.js",
        "service/lefigaro.service.js",
        "service/facebook.service.js",
        "service/orpi.service.js",
        "custom.js"
      ],
      "run_at": "document_end",
      "css": [
        "custom.css"
      ]
    }
  ],
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon-48x48.png",
    "128": "images/favicon-128x128.png"
  },
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "{c16ae65a-f869-4c8e-b00d-f46998ca63ec}"
    }
  }
}